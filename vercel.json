{
  "version": 2,
  "builds": [
    {
      "src": "simple_dashboard.py",
      "use": "@vercel/python"
    },
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "simple_dashboard.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "simple_dashboard.py": {
      "maxDuration": 30
    },
    "app/main.py": {
      "maxDuration": 30
    }
  }
}
